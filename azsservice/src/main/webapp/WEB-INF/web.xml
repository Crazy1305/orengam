<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>azsservice</display-name>
  <welcome-file-list>
    <welcome-file>action.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>PumpsServlet</display-name>
    <servlet-name>PumpsServlet</servlet-name>
    <servlet-class>ru.orengam.azsservice.servlets.PumpsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PumpsServlet</servlet-name>
    <url-pattern>/sale.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RefundServlet</display-name>
    <servlet-name>RefundServlet</servlet-name>
    <servlet-class>ru.orengam.azsservice.servlets.RefundServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RefundServlet</servlet-name>
    <url-pattern>/refund.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CardType</display-name>
    <servlet-name>CardType</servlet-name>
    <servlet-class>ru.orengam.service.servlets.CardType</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CardType</servlet-name>
    <url-pattern>/cardtype.html</url-pattern>
  </servlet-mapping>
</web-app>